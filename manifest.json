{
"manifest_version": 3,
"name": "FocusLock",
"version": "1.0",
"description": "Lock yourself into a specified website and close distracting tabs if you stray.",
"permissions": [
"tabs",
"activeTab",
"storage",
"notifications"
],
"host_permissions": ["<all_urls>"],
"background": {
"service_worker": "background.js"
},
"action": {
"default_popup": "popup.html",
"default_title": "FocusLock"
}
}